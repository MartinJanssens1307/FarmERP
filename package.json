{
  "name": "erpfarm",
  "version": "1.0.0",
  "description": "ERP Farm Management Application",
  "main": "index.js",
  "scripts": {
    "css:watch": "npx tailwindcss -i ERP/static/src/input.css -o ERP/static/css/styles.css --watch --content './**/*.{html,py,css}' --poll",
    "css:build": "npx tailwindcss -i ERP/static/src/input.css -o ERP/static/css/styles.css --minify --content './**/*.{html,py,css}'"
  },
  "keywords": [
    "django",
    "tailwind"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "autoprefixer": "^10.4.19",
    "postcss": "^8.4.38",
    "postcss-cli": "^11.0.0",
    "tailwindcss": "^3.4.1"
  }
}